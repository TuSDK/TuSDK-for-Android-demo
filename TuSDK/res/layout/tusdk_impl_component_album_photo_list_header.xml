<?xml version="1.0" encoding="utf-8"?>
<org.lasque.tusdk.impl.components.album.TuPhotoListHeader xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="44dp"
    android:background="@color/lsq_background_default_header"
    android:paddingBottom="4dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp" >

    <TextView
        android:id="@+id/lsq_dateLabel"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="bottom"
        android:text="@string/lsq_text"
        android:textColor="@color/lsq_color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/lsq_weekLabel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/lsq_dateLabel"
        android:gravity="bottom"
        android:paddingLeft="5dp"
        android:text="@string/lsq_text"
        android:textColor="@color/lsq_text_group_header"
        android:textSize="11sp" />

</org.lasque.tusdk.impl.components.album.TuPhotoListHeader>